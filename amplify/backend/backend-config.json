{
  "storage": {
    "CatsDb2": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "lambdaForCats2": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "CatsDb2",
          "attributes": [
            "Name",
            "Arn",
            "StreamArn"
          ]
        }
      ]
    },
    "lambdaForThumbnail": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "lambdaRekognition": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "auth": {},
  "api": {
    "catsApi2": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "lambdaForCats2",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  }
}